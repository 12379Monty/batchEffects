@article{Meinshausen:2010aa,
	annote = {doi: 10.1111/j.1467-9868.2010.00740.x},
	author = {Meinshausen, Nicolai and B{\"u}hlmann, Peter},
	booktitle = {Journal of the Royal Statistical Society: Series B (Statistical Methodology)},
	da = {2010/09/01},
	date = {2010/09/01},
	date-added = {2020-10-15 11:59:29 -0700},
